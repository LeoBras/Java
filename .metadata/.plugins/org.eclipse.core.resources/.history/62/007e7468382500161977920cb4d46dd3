import static org.junit.Assert.*;

import org.junit.Test;

public class Produto_test {
	
	@Test
	public void testHash() {
		Produto p = new Produto("Carrinho",1000, 50.0);
		assertEquals(p.hashCode(), p.codigo);
	}
	
	@Test
	public void testEquals() {
		Produto p = new Produto("Carrinho",1000, 50.0);
		assertEquals(p.hashCode(), p.codigo);
	}
	
	

}
