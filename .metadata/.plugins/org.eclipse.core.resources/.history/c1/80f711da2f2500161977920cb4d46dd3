
public class Produto {
	private String nome;
	private int codigo;
	private double preco;
	
	
	public void igual(){
		Produto p;
		p.hashCode();
	}
	
	public boolean equals(Object obj){
		Produto prod = (Produto) obj;
		if (this.codigo == prod.codigo)
			return true;
		
		return false;
	}
	
	public int hashCode(){
		return codigo;
	}
}
